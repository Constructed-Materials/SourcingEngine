{
  "Database": {
    "ConnectionString": "",
    "MaxConcurrentSchemaQueries": 5
  },
  "SemanticSearch": {
    "Enabled": true,
    "DefaultMode": "FamilyFirst",
    "FullTextWeight": 1.0,
    "SemanticWeight": 1.0,
    "RrfK": 50,
    "MatchCount": 5,
    "SimilarityThreshold": 0.4
  },
  "Bedrock": {
    "Enabled": true,
    "Region": "us-east-2",
    "EmbeddingModelId": "amazon.titan-embed-text-v2:0",
    "EmbeddingDimension": 1024,
    "EmbeddingNormalize": true,
    "ParsingModelId": "us.amazon.nova-lite-v1:0",
    "ParsingMaxTokens": 500,
    "ParsingTemperature": 0.1,
    "TimeoutSeconds": 30,
    "MaxConcurrentEmbeddings": 5
  },
  "Lambda": {
    "BrokerHost": "",
    "BrokerPort": 5671,
    "BrokerSecretArn": "",
    "BrokerUseSsl": true,
    "ResultExchange": "sourcing.engine",
    "ResultRoutingKey": "search.result",
    "ZeroResultRoutingKey": "search.zero-result"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "SourcingEngine": "Debug"
    }
  }
}
