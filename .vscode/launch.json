{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Generate Embeddings (All)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder:SourcingEngine}/src/SourcingEngine.Console/bin/Debug/net9.0/SourcingEngine.Console.dll",
            "args": ["--generate-embeddings", "--all"],
            "cwd": "${workspaceFolder:SourcingEngine}/src/SourcingEngine.Console",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Search Query",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder:SourcingEngine}/src/SourcingEngine.Console/bin/Debug/net9.0/SourcingEngine.Console.dll",
            "args": ["8 inch masonry block"],
            "cwd": "${workspaceFolder:SourcingEngine}/src/SourcingEngine.Console",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Lambda: Local RabbitMQ Consumer",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder:SourcingEngine}/src/SourcingEngine.BomExtraction.Lambda.LocalRunner/bin/Debug/net9.0/SourcingEngine.BomExtraction.Lambda.LocalRunner.dll",
            "cwd": "${workspaceFolder:SourcingEngine}/src/SourcingEngine.BomExtraction.Lambda.LocalRunner",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "env": {
                "AWS_PROFILE": "default",
                "AWS_REGION": "us-east-2"
            }
        },
        {
            "name": "Lambda: Replay Event File",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder:SourcingEngine}/src/SourcingEngine.BomExtraction.Lambda.LocalRunner/bin/Debug/net9.0/SourcingEngine.BomExtraction.Lambda.LocalRunner.dll",
            "args": ["--event", "${workspaceFolder:SourcingEngine}/src/SourcingEngine.BomExtraction.Lambda/local/test-event-template.json"],
            "cwd": "${workspaceFolder:SourcingEngine}/src/SourcingEngine.BomExtraction.Lambda.LocalRunner",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "env": {
                "AWS_PROFILE": "default",
                "AWS_REGION": "us-east-2"
            }
        }
    ]
}
